{
	"_version": "1.12.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.cgi.workshop.fiori",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"dataSources": {
			"customersData": {
				"uri": "model/data.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "com.cgi.workshop.fiori.view.App",
			"type": "XML",
			"async": true,
			"id": "main"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.cgi.workshop.fiori.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "customersData"
			}
		},
		"contentDensities": {
			"cozy": true,
			"compact": false
		}
	}
}